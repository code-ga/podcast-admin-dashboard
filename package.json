{
  "name": "podcast-admin-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run prisma:generate && npm run prisma:push; next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:push": "prisma db push",
    "prisma:generate": "prisma generate",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "next": "^14.0.3",
    "podcast": "^2.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "eslint": "8.41.0",
    "typescript": "5.0.4",
    "eslint-config-next": "13.4.4",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5"
  }
}
